<access_mechanisms>
  


      <access_mechanism accessor="MRS AMCG1IDR_EL0">
        <encoding>
          
          <access_instruction>MRS &lt;Xt&gt;, AMCG1IDR_EL0</access_instruction>
            
            <enc n="op0" v="0b11"/>
            
