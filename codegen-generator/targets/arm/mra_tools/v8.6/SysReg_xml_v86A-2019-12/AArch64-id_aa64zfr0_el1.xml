      <access_mechanism accessor="MRS ID_AA64ZFR0_EL1">
        <encoding>
          
          <access_instruction>MRS &lt;Xt&gt;, ID_AA64ZFR0_EL1</access_instruction>
            
            <enc n="op0" v="0b11"/>
            
