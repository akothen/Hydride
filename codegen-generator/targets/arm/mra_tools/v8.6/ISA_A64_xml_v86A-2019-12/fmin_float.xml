case operation of
    when <a link="FPMaxMinOp_MAX" file="shared_pseudocode.xml" hover="enumeration FPMaxMinOp  {FPMaxMinOp_MAX, FPMaxMinOp_MIN,&#13; FPMaxMinOp_MAXNUM, FPMaxMinOp_MINNUM}">FPMaxMinOp_MAX</a>    result = <a link="impl-shared.FPMax.3" file="shared_pseudocode.xml" hover="function: bits(N) FPMax(bits(N) op1, bits(N) op2, FPCRType fpcr)">FPMax</a>(operand1, operand2, FPCR);
    when <a link="FPMaxMinOp_MIN" file="shared_pseudocode.xml" hover="enumeration FPMaxMinOp  {FPMaxMinOp_MAX, FPMaxMinOp_MIN,&#13; FPMaxMinOp_MAXNUM, FPMaxMinOp_MINNUM}">FPMaxMinOp_MIN</a>    result = <a link="impl-shared.FPMin.3" file="shared_pseudocode.xml" hover="function: bits(N) FPMin(bits(N) op1, bits(N) op2, FPCRType fpcr)">FPMin</a>(operand1, operand2, FPCR);
    when <a link="FPMaxMinOp_MAXNUM" file="shared_pseudocode.xml" hover="enumeration FPMaxMinOp  {FPMaxMinOp_MAX, FPMaxMinOp_MIN,&#13; FPMaxMinOp_MAXNUM, FPMaxMinOp_MINNUM}">FPMaxMinOp_MAXNUM</a> result = <a link="impl-shared.FPMaxNum.3" file="shared_pseudocode.xml" hover="function: bits(N) FPMaxNum(bits(N) op1, bits(N) op2, FPCRType fpcr)">FPMaxNum</a>(operand1, operand2, FPCR);
    when <a link="FPMaxMinOp_MINNUM" file="shared_pseudocode.xml" hover="enumeration FPMaxMinOp  {FPMaxMinOp_MAX, FPMaxMinOp_MIN,&#13; FPMaxMinOp_MAXNUM, FPMaxMinOp_MINNUM}">FPMaxMinOp_MINNUM</a> result = <a link="impl-shared.FPMinNum.3" file="shared_pseudocode.xml" hover="function: bits(N) FPMinNum(bits(N) op1, bits(N) op2, FPCRType fpcr)">FPMinNum</a>(operand1, operand2, FPCR);

<a link="impl-aarch64.V.write.1" file="shared_pseudocode.xml" hover="accessor: V[integer n] = bits(width) value">V</a>[d] = result;</pstext>
    </ps>
