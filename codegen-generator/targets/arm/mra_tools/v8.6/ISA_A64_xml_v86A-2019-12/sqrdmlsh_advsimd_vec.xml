        <ps name="aarch64/instrs/vector/arithmetic/binary/uniform/mul/int/doubling/accum/sisd" mylink="aarch64.instrs.vector.arithmetic.binary.uniform.mul.int.doubling.accum.sisd" enclabels="" sections="1" secttype="noheading">
          <pstext mayhavelinks="1" section="Decode" rep_section="decode">if !<a link="impl-shared.HaveQRDMLAHExt.0" file="shared_pseudocode.xml" hover="function: boolean HaveQRDMLAHExt()">HaveQRDMLAHExt</a>() then UNDEFINED;

integer d = <a link="impl-shared.UInt.1" file="shared_pseudocode.xml" hover="function: integer UInt(bits(N) x)">UInt</a>(Rd);
integer n = <a link="impl-shared.UInt.1" file="shared_pseudocode.xml" hover="function: integer UInt(bits(N) x)">UInt</a>(Rn);
