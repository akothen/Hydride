    [(cpp:signed-expr? lhs)
     (mk-cpp-expr (if (bveq (cpp:eval lhs) (cpp:eval rhs)) (bv #b1 1) (bv #b0 1)) 'uint1)]
    [else
     (mk-cpp-expr (if (bveq (cpp:eval lhs) (cpp:eval rhs)) (bv #b1 1) (bv #b0 1)) 'uint1)]))

